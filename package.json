{
  "name": "io-ts-jsend",
  "version": "0.1.0",
  "description": "JSend utility codecs for use with the io-ts library",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.es6.json",
    "lint": "tslint -p tsconfig.tslint.json src/**/*.ts test/**/*.ts",
    "jest-clear-cache": "jest --clearCache",
    "jest": "jest --ci",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest"
  },
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/huntwj/io-ts-jsend.git"
  },
  "keywords": [
    "io",
    "codec",
    "fp",
    "jsend"
  ],
  "author": "Wil Hunt <huntwj@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huntwj/io-ts-jsend/issues"
  },
  "homepage": "https://github.com/huntwj/io-ts-jsend#readme",
  "dependencies": {
    "fp-ts": "^2.5.3",
    "io-ts": "^2.1.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "trailingComma": "all"
  }
